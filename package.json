{
  "name": "nuanalytics",
  "version": "0.1.0",
  "homepage": "https://github.com/NeuCurricularAnalytics/NuAnalytics#readme",
  "bugs": {
    "url": "https://github.com/NeuCurricularAnalytics/NuAnalytics/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeuCurricularAnalytics/NuAnalytics.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "cargo build",
    "build": "cargo build --release",
    "run": "cargo run --package nu_analytics --bin nuanalytics",
    "lint": "npm run lint:clippy && npm run lint:fmt && npm run lint:doc",
    "lint:clippy": "cargo clippy --all-targets --all-features --workspace -- -D warnings",
    "lint:fmt": "cargo fmt --all -- --check",
    "lint:doc": "cargo doc --no-deps --all-features",
    "test": "cargo test --workspace",
    "doc": "cargo doc --no-deps --document-private-items --all-features && rm -rf docs/rust && mv target/doc docs/rust",
    "clean": "cargo clean"
  },
  "devDependencies": {}
}
